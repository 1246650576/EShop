<generatorConfiguration>

    <properties resource="database.properties"></properties>

    <!--数据库驱动-->
    <classPathEntry location="E:\\Apache\\apache-maven-3.5.2\\repository\\mysql\\mysql-connector-java\\8.0.18\\mysql-connector-java-8.0.18.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
